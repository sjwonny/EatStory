<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="brand">
	
	<select id="selectList" resultType="brand">
		select * from brand
	</select>


	 <select id="selectBrandName" parameterType="int" resultType="brand">
		select name from brand where no = #{no}
	</select>
	 
</mapper>








































